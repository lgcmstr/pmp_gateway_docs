<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || 'NARxCHECK API Documentation' %></title>
    <link href='https://fonts.googleapis.com/css?family=Source+Code+Pro|Source+Sans+Pro:400,300,600' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag 'normalize', 'all' %>
    <%= javascript_include_tag 'all' %>
  </head>
  <body>
    <%= partial 'side_nav' %>
    <%= yield %>
  </body>
</html>
